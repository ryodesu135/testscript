        local char = player.Character
        local humanoid = char and char:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = Value
            getgenv().UserSetWalkSpeed = Value
            print("[DEBUG] ç©å®¶è®¾å®š WalkSpeed:", Value)
        end
    end,
})

